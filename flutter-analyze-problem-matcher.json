{
    "problemMatcher": [
        {
            "owner": "flutter-analyze",
            "pattern": [
                {
                    "regexp": "^Analyzing "
                },
                {
                    "regexp": "^\\s+info • (.+) • (.+):(\\d+):(\\d+) • (.*)$",
                    "message": 1,
                    "file": 2,
                    "line": 3,
                    "column": 4,
                    "code": 5,
                    "loop": true
                }
            ]
        }
    ]
}